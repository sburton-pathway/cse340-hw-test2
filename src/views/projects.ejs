<%- include('partials/header') %>
    <main>
        <h1><%= title %></h1>

        <h2>Upcoming Service Projects</h2>
        <ul>
        <% projects.forEach(project => { %>
            <li><%= new Date(project.date).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) %>: <%= project.title %> (<%= project.organization_name %>)</li>
        <% }); %>

        </ul> 
    </main>   

<%- include('partials/footer') %>