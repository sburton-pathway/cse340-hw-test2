<%- include('partials/header') %>
    <main>
        <h1><%= title %></h1>

        <h2>Upcoming Service Projects</h2>
        <ul>
        <% projects.forEach(project => { %>
            <li><%= new Date(project.date).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) %>: <a href="/project/<%= project.project_id %>"><%= project.title %></a> (<a href="/organization/<%= project.organization_id %>"><%= project.organization_name %></a>)</li>
        <% }); %>

        </ul> 
    </main>   

<%- include('partials/footer') %>